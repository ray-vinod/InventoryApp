@page "/"
@namespace InventoryApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>InventoryApp</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="InventoryApp.styles.css" rel="stylesheet" />
    <link href="_content/BlazorPro.Spinkit/spinkit.min.css" rel="stylesheet" />


</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

     <footer class="page-footer font-small border-top text-muted cyan darken-3 ">
        <div class="footer-copyright text-center py-3 footer">
            © @DateTime.Now.Date.Year.ToString() Copyright - CIWEC Hospital Pvt. Ltd.
        </div>
    </footer>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
